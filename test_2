#------------------------------------------------------
function ini() {
clear
sel=$(whiptail --title " INI " --menu "\
\n
" 17 45 9 \
"1" "var.txt" \
"2" "func.txt" \
"3" "MMDVM_Bridge.ini" \
"4" "Analog_Bridge.ini" \
"5" "DVSwitch.ini" \
"6" "dvswitch.sh" \
"7" "dvsm.macro" \
"8" "dvsm.sh" \
"9" "dvswitch.sh" \
3>&1 1>&2 2>&3)

if [ $? != 0 ]; then ${DVS}dvsman; exit 0; fi

case $sel in
1)
sudo nano /var/lib/dvswitch/dvs/var.txt; ${DVS}dvsman ini; exit 0 ;;
2)
sudo nano /var/lib/dvswitch/dvs/func.txt; ${DVS}dvsman ini; exit 0 ;;
3)
sudo nano /opt/MMDVM_Bridge/MMDVM_Bridge.ini; ${DVS}dvsman ini; exit 0 ;;
4)
sudo nano /opt/Analog_Bridge/Analog_Bridge.ini; ${DVS}dvsman ini; exit 0 ;;
5)
sudo nano /opt/MMDVM_Bridge/DVSwitch.ini; ${DVS}dvsman ini; exit 0 ;;
6)
sudo nano /opt/MMDVM_Bridge/dvswitch.sh; ${DVS}dvsman ini; exit 0 ;;
7)
sudo nano /opt/Analog_Bridge/dvsm.macro; ${DVS}dvsman ini; exit 0 ;;
8)
sudo nano /opt/Analog_Bridge/dvsm.sh; ${DVS}dvsman ini; exit 0 ;;
9)
sudo nano /opt/MMDVM_Bridge/dvswitch.sh; ${DVS}dvsman ini; exit 0 ;;
esac
}

#------------------------------------------------------
function dvs_file() {
clear
sel=$(whiptail --title " DVS FILES " --menu "\
\n
" 30 45 22 \
"1" "88_restart.sh" \
"2" "99_reboot.sh" \
"3" "adhoc_adv.sh" \
"4" "adhoc_basic.sh" \
"5" "adhoc_check.sh" \
"6" "adnl_dmr.sh" \
"7" "adv_config_menu.sh" \
"8" "auto_upgrade.sh" \
"9" "clone.sh" \
"10" "config_ini.sh" \
"11" "DMRIds_chk.sh" \
"12" "dvs" \
"13" "dvsb" \
"14" "init_config,sh" \
"15" "language,sh" \
"16" "man_log" \
"17" "temp_msg,sh" \
"18" "temp,sh" \
"19" "tgdb,sh" \
"20" "tools_menu,sh" \
"21" "update_upgrade,sh" \
"22" "upgrade,sh" \
3>&1 1>&2 2>&3)

if [ $? != 0 ]; then ${DVS}dvsman; exit 0; fi

case $sel in
1)
sudo nano /usr/local/dvs/88_restart.sh; ${DVS}dvsman dvs_file; exit 0 ;;
2)
sudo nano /usr/local/dvs/99_reboot.sh; ${DVS}dvsman dvs_file; exit 0 ;;
3)
sudo nano /usr/local/dvs/adhoc_adv.sh; ${DVS}dvsman dvs_file; exit 0 ;;
4)
sudo nano /usr/local/dvs/adhoc_basic.sh; ${DVS}dvsman dvs_file; exit 0 ;;
5)
sudo nano /usr/local/dvs/adhoc_check.sh; ${DVS}dvsman dvs_file; exit 0 ;;
6)
sudo nano /usr/local/dvs/adnl_dmr.sh; ${DVS}dvsman dvs_file; exit 0 ;;
7)
sudo nano /usr/local/dvs/adv_config_menu.sh; ${DVS}dvsman dvs_file; exit 0 ;;
8)
sudo nano /usr/local/dvs/auto_upgrade.sh; ${DVS}dvsman dvs_file; exit 0 ;;
9)
sudo nano /usr/local/dvs/clone.sh; ${DVS}dvsman dvs_file; exit 0 ;;
10)
sudo nano /usr/local/dvs/config_ini.sh; ${DVS}dvsman dvs_file; exit 0 ;;
11)
sudo nano /usr/local/dvs/DMRIds_chk.sh; ${DVS}dvsman dvs_file; exit 0 ;;
12)
sudo nano /usr/local/dvs/dvs; ${DVS}dvsman dvs_file; exit 0 ;;
13)
sudo nano /usr/local/dvs/dvsb; ${DVS}dvsman dvs_file; exit 0 ;;
14)
sudo nano /usr/local/dvs/init_config.sh; ${DVS}dvsman dvs_file; exit 0 ;;
15)
sudo nano /usr/local/dvs/language.sh; ${DVS}dvsman dvs_file; exit 0 ;;
16)
sudo nano /usr/local/dvs/man_log; ${DVS}dvsman dvs_file; exit 0 ;;
17)
sudo nano /usr/local/dvs/temp_msg.sh; ${DVS}dvsman dvs_file; exit 0 ;;
18)
sudo nano /usr/local/dvs/temp.sh; ${DVS}dvsman dvs_file; exit 0 ;;
19)
sudo nano /usr/local/dvs/tgdb.sh; ${DVS}dvsman dvs_file; exit 0 ;;
20)
sudo nano /usr/local/dvs/tools_menu.sh; ${DVS}dvsman dvs_file; exit 0 ;;
21)
sudo nano /usr/local/dvs/update_upgrade.sh; ${DVS}dvsman dvs_file; exit 0 ;;
22)
sudo nano /usr/local/dvs/upgrade.sh; ${DVS}dvsman dvs_file; exit 0 ;;
esac
}

#-------------------------------------------------------------
function log() {
clear
sel=$(whiptail --title " 로그 확인 " --menu "\
\n
" 15 50 6 \
"1" "dvsmu.log 열기" \
"2" "auto_upgrade.log 열기" \
"3" "bm_watchdog.txt 열기" \
"4" "dvsstart-runner.sh 열기" \
"5" "crontab 열기" \
"6" "Back to Main" \
3>&1 1>&2 2>&3)

if [ $? != 0 ]; then ${DVS}dvsman; exit 0
fi

case $sel in
1)
if [ ! -e /var/log/dvswitch/dvsmu.log ]; then
        echo "This is a Log file for dvsMU" | sudo tee /var/log/dvswitch/dvsmu.log > /dev/null 2>&1
        log_line=--------------------------------------------------
        sudo sed -i "1 i\\$log_line" /var/log/dvswitch/dvsmu.log
fi
sudo nano /var/log/dvswitch/dvsmu.log; ${DVS}dvsman log; exit 0 ;;
2)
sudo nano /var/log/dvswitch/auto_upgrade.log; ${DVS}dvsman log; exit 0 ;;
3)
sudo nano /var/log/dvswitch/bm_watchdog.log; ${DVS}dvsman log; exit 0 ;;
4)
sudo nano /var/log/dvswitch/dvsstart-run.log; ${DVS}dvsman log; exit 0 ;;
5)
sudo nano /etc/crontab; ${DVS}dvsman log; exit 0 ;;
6)
${DVS}dvsman; exit 0 ;;
esac
}


#== MAIN SCRIPT ============================================
clear

if [ "$1" = lan ]; then lan; fi
if [ "$1" = tgdb ]; then tgdb; fi
if [ "$1" = tgdb_en ]; then tgdb_en; fi
if [ "$1" = tgdb_kr ]; then tgdb_kr; fi
if [ "$1" = adv ]; then adv; fi
if [ "$1" = adv_en ]; then adv_en; fi
if [ "$1" = adv_kr ]; then adv_kr; fi
if [ "$1" = host ]; then host; fi
if [ "$1" = ini ]; then ini; fi
if [ "$1" = dvs_file ]; then dvs_file; fi
if [ "$1" = sys ]; then sys; fi
if [ "$1" = log ]; then log; fi
if [ "$1" = bm_test ]; then bm_watchdog_test; fi

sel=$(whiptail --title " DVS Management " --menu "\
\n
" 18 45 10 \
"1" "DVS FILES" \
"2" "INI FILES" \
"3" "LANGUAGE" \
"4" "TGDB" \
"5" "ADV" \
"6" "HOST" \
"7" "SYS" \
"8" "LOG" \
"9" "crontab" \
"10" "bm_watchdog TEST" \
3>&1 1>&2 2>&3)

case $sel in
1)
dvs_file; ${DVS}dvsman; exit 0 ;;
2)
ini; ${DVS}dvsman; exit 0 ;;
3)
lan; ${DVS}dvsman; exit 0 ;;
4)
tgdb; ${DVS}dvsman; exit 0 ;;
5)
adv; ${DVS}dvsman; exit 0 ;;
6)
host; ${DVS}dvsman; exit 0 ;;
7)
sys; ${DVS}dvsman; exit 0 ;;
8)
log; ${DVS}dvsman; exit 0 ;;
9)
sudo nano /etc/crontab; ${DVS}dvsman; exit 0 ;;
10)
bm_watchdog_test; ${DVS}dvsman; exit 0 ;;
esac






