#------------------------------------------------------
function tgdb() {
clear
sel=$(whiptail --title " TGDB " --menu "\
\n
" 20 45 12 \
"1" "DMR_fvrt_list.txt" \
"2" "DMR_node_list.txt" \
"3" "DSTAR_fvrt_list.txt" \
"4" "DSTAR_node_list.txt" \
"5" "NXDN_fvrt_list.txt" \
"6" "NXDN_node_list.txt" \
"7" "P25_fvrt_list.txt" \
"8" "P25_node_list.txt" \
"9" "YSF_fvrt_list.txt" \
"10" "YSF_node_list.txt" \
"11" "TGDB EN ----->" \
"12" "TGDB KR ----->" \
3>&1 1>&2 2>&3)

if [ $? != 0 ]; then ${DVS}dvsman; exit 0; fi

case $sel in
1)
sudo nano /var/lib/dvswitch/dvs/tgdb/DMR_fvrt_list.txt; ${DVS}dvsman tgdb; exit 0 ;;
2)
sudo nano /var/lib/dvswitch/dvs/tgdb/DMR_node_list.txt; ${DVS}dvsman tgdb; exit 0 ;;
3)
sudo nano /var/lib/dvswitch/dvs/tgdb/DSTAR_fvrt_list.txt; ${DVS}dvsman tgdb; exit 0 ;;
4)
sudo nano /var/lib/dvswitch/dvs/tgdb/DSTAR_node_list.txt; ${DVS}dvsman tgdb; exit 0 ;;
5)
sudo nano /var/lib/dvswitch/dvs/tgdb/NXDN_fvrt_list.txt; ${DVS}dvsman tgdb; exit 0 ;;
6)
sudo nano /var/lib/dvswitch/dvs/tgdb/NXDN_node_list.txt; ${DVS}dvsman tgdb; exit 0 ;;
7)
sudo nano /var/lib/dvswitch/dvs/tgdb/P25_fvrt_list.txt; ${DVS}dvsman tgdb; exit 0 ;;
8)
sudo nano /var/lib/dvswitch/dvs/tgdb/P25_node_list.txt; ${DVS}dvsman tgdb; exit 0 ;;
9)
sudo nano /var/lib/dvswitch/dvs/tgdb/YSF_fvrt_list.txt; ${DVS}dvsman tgdb; exit 0 ;;
10)
sudo nano /var/lib/dvswitch/dvs/tgdb/YSF_node_list.txt; ${DVS}dvsman tgdb; exit 0 ;;
11)
tgdb_en; exit 0 ;;
12)
tgdb_kr; exit 0 ;;
esac
}


