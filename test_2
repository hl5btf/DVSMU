#!/bin/bash

source /var/lib/dvswitch/dvs/var.txt

#------------------------------------------------------
function bm_watchdog_test() {
clear
sel=$(whiptail --title " bm_watchdog_test " --menu "\
\n
" 18 45 10 \
"1" "DMR_Hosts.txt" \
"2" "bm_present_list.txt" \
"3" "bm_watchdog_status.txt" \
"4" "reserved" \
"5" "실행" \
"6" "var.txt" \
"7" "var01.txt" \
"8" "var03.txt" \
"9" "var05.txt" \
"10" "LOG" \
3>&1 1>&2 2>&3)

if [ $? != 0 ]; then ${DVS}dvsman; exit 0; fi

case $sel in
1)
sudo nano /var/lib/mmdvm/DMR_Hosts.txt; ${DVS}dvsman bm_test; exit 0 ;;
2)
sudo nano /var/tmp/bm_present_list.txt; ${DVS}dvsman bm_test; exit 0 ;;
3)
sudo nano /var/tmp/bm_watchdog_status.txt; ${DVS}dvsman bm_test; exit 0 ;;
4)
sudo nano /var/lib/mmdvm/DMR_Hosts.txt; ${DVS}dvsman bm_test; exit 0 ;;
5)
${DVS}dvsman bm_test; exit 0 ;;
6)
sudo nano /var/lib/dvswitch/dvs/var.txt; ${DVS}dvsman bm_test; exit 0 ;;
7)
sudo nano /var/lib/dvswitch/dvs/var01.txt; ${DVS}dvsman bm_test; exit 0 ;;
8)
sudo nano /var/lib/dvswitch/dvs/var03.txt; ${DVS}dvsman bm_test; exit 0 ;;
9)
sudo nano /var/lib/dvswitch/dvs/var05.txt; ${DVS}dvsman bm_test; exit 0 ;;
10)
sudo nano /var/log/dvswitch/bm_watchdog.log; ${DVS}dvsman bm_test; exit 0 ;;
esac
}
#------------------------------------------------------
function lan() {
clear
sel=$(whiptail --title " LANGUAGE " --menu "\
\n
" 16 45 8 \
"1" "template.txt" \
"2" "korean.txt" \
"3" "english.txt" \
"4" "japanese.txt" \
"5" "german.txt" \
"6" "greek.txt" \
"7" "polish.txt" \
"8" "language.txt" \
3>&1 1>&2 2>&3)

if [ $? != 0 ]; then ${DVS}dvsman; exit 0; fi

case $sel in
1)
sudo nano /var/lib/dvswitch/dvs/lan/template.txt; ${DVS}dvsman lan; exit 0 ;;
2)
sudo nano /var/lib/dvswitch/dvs/lan/korean.txt; ${DVS}dvsman lan; exit 0 ;;
3)
sudo nano /var/lib/dvswitch/dvs/lan/english.txt; ${DVS}dvsman lan; exit 0 ;;
4)
sudo nano /var/lib/dvswitch/dvs/lan/japanese.txt; ${DVS}dvsman lan; exit 0 ;;
5)
sudo nano /var/lib/dvswitch/dvs/lan/german.txt; ${DVS}dvsman lan; exit 0 ;;
6)
sudo nano /var/lib/dvswitch/dvs/lan/greek.txt; ${DVS}dvsman lan; exit 0 ;;
7)
sudo nano /var/lib/dvswitch/dvs/lan/polish.txt; ${DVS}dvsman lan; exit 0 ;;
8)
sudo nano /var/lib/dvswitch/dvs/lan/language.txt; ${DVS}dvsman lan; exit 0 ;;
esac
}

#------------------------------------------------------
function tgdb_en() {
clear
sel=$(whiptail --title " TGDB EN " --menu "\
\n
" 13 45 5 \
"1" "DMR_fvrt_list.txt" \
"2" "DSTAR_fvrt_list.txt" \
"3" "NXDN_fvrt_list.txt" \
"4" "P25_fvrt_list.txt" \
"5" "YSF_fvrt_list.txt" \
3>&1 1>&2 2>&3)

if [ $? != 0 ]; then ${DVS}dvsman tgdb; exit 0; fi

case $sel in
1)
sudo nano /var/lib/dvswitch/dvs/tgdb/EN/DMR_fvrt_list.txt; ${DVS}dvsman tgdb_en; exit 0 ;;
2)
sudo nano /var/lib/dvswitch/dvs/tgdb/EN/DSTAR_fvrt_list.txt; ${DVS}dvsman tgdb_en; exit 0 ;;
3)
sudo nano /var/lib/dvswitch/dvs/tgdb/EN/NXDN_fvrt_list.txt; ${DVS}dvsman tgdb_en; exit 0 ;;
4)
sudo nano /var/lib/dvswitch/dvs/tgdb/EN/P25_fvrt_list.txt; ${DVS}dvsman tgdb_en; exit 0 ;;
5)
sudo nano /var/lib/dvswitch/dvs/tgdb/EN/YSF_fvrt_list.txt; ${DVS}dvsman tgdb_en; exit 0 ;;
esac
}

