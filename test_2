#------------------------------------------------------
function adv() {
clear
sel=$(whiptail --title " ADV " --menu "\
\n
" 12 45 4 \
"1" "dvsm.macro" \
"2" "dvsm.sh" \
"3" "EN ----->" \
"4" "KR ----->" \
3>&1 1>&2 2>&3)

if [ $? != 0 ]; then ${DVS}dvsman; exit 0; fi

case $sel in
1)
sudo nano /var/lib/dvswitch/dvs/adv/dvsm.macro;  ${DVS}dvsman adv; exit 0 ;;
2)
sudo nano /var/lib/dvswitch/dvs/adv/dvsm.sh;  ${DVS}dvsman adv; exit 0 ;;
3)
adv_en; exit 0 ;;
4)
adv_kr; exit 0 ;;
esac
}

